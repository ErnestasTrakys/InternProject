<mvc:View controllerName="visionBoardProject.internproject.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">

           
            <App>
        <pages>
            <Page title="{i18n>title}" >
                <content>
                    <Panel headerText="Hello">
                    <content>
                    <Table 
                                id="userSelectionTable"
                                items="{/value}">
                                <columns>
                                    <Column>
                                        <Button text="{i18n>Upload}" press=".onSubmitRoute" />
                                    </Column>
                                    <Column>
                                        <Button text="{i18n>Friends}" press=".onSubmitRoute" />
                                    </Column>
                                    <Column>
                                        <Button text="{i18n>Notifications}" press=".onSubmitRoute" />
                                    </Column>
                                    <Column>
                                        <Button text="{i18n>Profile}" press=".onSubmitRoute" />
                                    </Column>
                                    <Column>
                                        <Button text="{i18n>Settings}" press=".onSubmitRoute" />
                                    </Column>
                                </columns>
                                <SearchField liveChange=".onSearch"/>
                               
                            </Table>
                            <SearchField liveChange=".onSearch"/>
                                <FlexBox direction="Column" class="sapUiMediumMarginTop">
                                    <FlexBox alignItems="Center" justifyContent="End" class="pageControls" >
                                        <ObjectStatus id="searchStatus" text="" />
                                        <Button text="Search" press=".onSubmitSearch" />                  
                                    </FlexBox>
                                </FlexBox>
                                
                                
                           
                        
                        </content>
                    </Panel>
                    
                </content>
            </Page>
        </pages>
    </App>

  
</mvc:View>








