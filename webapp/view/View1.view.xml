<mvc:View controllerName="visionBoardProject.internproject.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">

           
            <App>
        <pages>
            <Page title="{i18n>title}" >
                <content>
                    <Panel headerText="Hello">
                    <content>
                    <SearchField liveChange=".onSearch"/>
                    <Table 
                                id="userSelectionTable"
                                items="{/value}">
                                <columns>
                                    <Column>
                                        <Text text="{i18n>Upload}" />
                                    </Column>
                                    <Column>
                                        <Text text="{i18n>Friends}" />
                                    </Column>
                                    <Column>
                                        <Text text="{i18n>Notifications}" />
                                    </Column>
                                    <Column>
                                        <Text text="{i18n>Profile}" />
                                    </Column>
                                    <Column>
                                        <Text text="{i18n>Settings}" />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem 
                                        vAlign="Middle"
                                        press=".onSelect"
                                        type="Active">
                                        
                                       
                                    </ColumnListItem>
                                </items>
                            </Table>
                                <!-- <NavBar>
                            <Public>
                                <ButtonLink>../ChapterMenu.aspx</ButtonLink>
                            </Public>
                                </NavBar> -->

                           
                        
                        </content>
                    </Panel>
                    
                </content>
            </Page>
        </pages>
    </App>

  
</mvc:View>








